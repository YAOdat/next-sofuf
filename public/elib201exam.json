        {
          "id": 24,
          "type": "multiple-choice",
          "text": "Just to ____, do you mean that the deadline has been updated?",
          "options": [
            { "value": "A", "label": "bargain" },
            { "value": "B", "label": "clarify" },
            { "value": "C", "label": "agree" }
